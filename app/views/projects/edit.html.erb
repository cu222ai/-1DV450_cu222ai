  <h1> Edit project: <%= @project.name %></h1>
  <%= form_for(@project) do |f| %>

<div class="text_field">
<%=f.label :name %>
<%=f.text_field :name %>
</div>

<div class="text_field">
<%=f.label :description %>
<%=f.text_area :description %>
</div>

<div class="text_field">
<%=f.label :start_date %>
<%=f.datetime_select :start_date %>
</div>


<div class="text_field">
<%=f.label :end_date %>
<%=f.datetime_select :end_date %>
</div>


<%= f.submit "Spara", :confirm => "Are you sure you want to update this project?"%>


<% end %>
